{
  "name": "react-puxl-icons",
  "version": "1.0.0",
  "description": "A react icons library based on the collection of accessible icons from Puxl framework",
  "author": "Adrián Bolonio <bolonio85@gmail.com>",
  "contributors": [
    "Adrián Bolonio <bolonio85@gmail.com>"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/bolonio/react-puxl-icons.git"
  },
  "bugs": {
    "url": "https://github.com/bolonio/react-puxl-icons/issues"
  },
  "homepage": "https://github.com/bolonio/react-puxl-icons#readme",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
